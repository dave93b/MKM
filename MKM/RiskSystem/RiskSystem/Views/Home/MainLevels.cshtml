@model List<MainLevel>

@{
    ViewBag.Title = "Головні рівні";
}

<h2>Головні рівні</h2>

@Html.ActionLink("Додати рівень","AddNewMainLevel")

<table class="table table-hover table-striped table-bordered">
    <tr>
        <th>Id</th>
        <th>Назва рівня</th>
    </tr>
    @foreach (var element in Model)
    {
        <tr>
            <td>@element.LevelId</td>
            <td>@element.LevelName</td>
        </tr>
    }
</table>